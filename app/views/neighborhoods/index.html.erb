<p id="notice"><%= notice %></p>

<h1>Listing Neighborhoods</h1>

<%= form_tag neighborhoods_path, :method => 'get' do %>
	 <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search", :name => nil, :class => "btn btn-primary" %>
<% end %>


<% @neighborhoods.each do |i| %>

<div class='col-md-2'> <%= link_to i.name , i %></div>

<% end %>
